{
  "name": "@toolbox/config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/config/src",
  "projectType": "library",
  "tags": ["scope:config"],
  "// targets": "to see all targets run: nx show project config --web",
  "targets": {
    "build": {
      "cache": true,
      "inputs": ["!{projectRoot}/**/*.md"],
      "outputs": ["{workspaceRoot}/packages/config/dist"]
    },
    "lint": {
      "cache": true,
      "inputs": ["{projectRoot}/**/*.{ts,tsx}"],
      "outputs": []
    }
  }
}
