  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugin-react/src",
  "projectType": "library",
  "tags": ["scope:plugin"],
  "// targets": "to see all targets run: nx show project @toolbox/plugin-react --web",
